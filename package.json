{
  "name": "@syook/react-tabulous",
  "version": "3.0.1",
  "description": "Terrifically fabulous React Table",
  "main": "lib/react-tabulous.min.js",
  "files": [
    "README.md",
    "lib/"
  ],
  "keywords": [
    "syook",
    "table",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syook/react-tabulous.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Syook",
  "bugs": {
    "url": "https://github.com/syook/react-tabulous/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "react-scripts start",
    "build": "mkdir -p lib && rm -rf lib/* && NODE_ENV=production webpack --config webpack.config.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "npm-publish": "npm i && npm run build && npm publish",
    "prettify": "prettier . -c --config .prettierrc",
    "prettify:fix": "prettier . -c --config .prettierrc --write"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "8.1.0",
    "css-loader": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-overlay": "6.0.9",
    "react-scripts": "^5.0.1",
    "react-overlays": "^5.2.0",
    "style-loader": "^1.3.0",
    "webpack": "5.75.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react-datepicker": "^4.6.0",
    "react-select": "^5.2.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.2"
  },
  "peerDependencies": {
    "react": "17",
    "react-dom": "17"
  }
}
